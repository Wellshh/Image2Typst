{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"]
      }
    },
    {
        "src": "public/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
} 