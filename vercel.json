{
  "version": 2,
  "builds": [
    {
      "src": "latex_to_typst/package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["latex_to_typst/**"]
      }
    },
    {
        "src": "latex_to_typst/public/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "latex_to_typst/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "latex_to_typst/public/$1"
    }
  ]
} 